#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,j;
    cin>>n;
    map<string,int> mp;
    while(n--)
    {
        string s;
        cin>>s;
        mp[s]++;
    }
    for(auto x:mp)
    {
        cout<<x.first<<" "<<x.second<<"\n";
    }
}
