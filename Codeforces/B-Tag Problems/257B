class BPlayingCubes {
public:
	void solve(std::istream& in, std::ostream& out) {
	    long long int n , m ;
	    in >> n >> m ;
	    out << (n+m) - min(n , m) - 1 << " " << min(n , m) << "\n" ;

	}
};


