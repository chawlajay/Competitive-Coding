public:
	void solve(std::istream& in, std::ostream& out) {
	    long long int t;
	    in >> t;
	    while(t-- > 0) {
	        long long int one = 0 , minus = 0 , x , y , n , i , flag = 0;
	        in >> n ;
	        long long int a[n+1] ;
	        for(i = 0 ; i < n ; i++) {
	            in >> a[i];
	        }
	        for(i = 0 ; i < n ; i++) {
	            in >> x;
	            if(i == 0) {
	                if(x != a[i]) {
	                    flag = 1;
//	                    break;
	                }
	            }
	            else {
	                if(x < a[i]) {
	                    if(minus == 0) {
	                        flag = 1;
//	                        break;
	                    }
	                }
	                else if(x > a[i]) {
	                    if(one == 0) {
	                        flag = 1;
//	                        break;
	                    }
	                }
	            }
	            if(a[i] == 1) {
	                one = 1;
	            }
	            else if(a[i] == -1) {
	                minus = 1;
	            }
	        }
	        if(flag) out << "NO\n";
	        else out << "YES\n";
	    }

	}
};


