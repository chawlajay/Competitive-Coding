#pragma comment(linker, "/stack:200000000")
#pragma GCC optimize("Ofast")
#pragma GCC target("sse,sse2,sse3,ssse3,sse4,popcnt,abm,mmx,avx,tune=native")

#include <bits/stdc++.h>

using namespace std;

class BCrosswordSolving {
public:
	void solve(std::istream& in, std::ostream& out) {
	    long long int n , m , i , j , ans = 0 , c , ind = -1 ;
	    in >> n >> m ;
	    string s , t ;
	    in >> t >> s ;
	    for(i = 0 ; i < m - n + 1 ; i++) {
	        c = 0 ;
	        for(j = 0 ; j < n ; j++) {
	            if(s[i+j] == t[j]) {
	                c++ ;
	            }
	        }
	        if(c > ans) {
                ans = max(ans, c);
                ind = i ;
            }

	    }
	    out << n - ans << "\n" ;
	    if(ind == -1) {
	        for(i = 1 ; i <= n ; i++) {
	            out << i << " " ;
	        }
	    }
	    else {
	        for(i = 0 ; i < n ; i++) {
	            if(s[ind + i] != t[i]) {
	                out << i+1 <<" " ;
	            }
	        }
	    }

	}
};


int main() {
	BCrosswordSolving solver;
	std::ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
