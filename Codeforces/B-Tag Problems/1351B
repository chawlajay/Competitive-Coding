class BSquare {
public:
	void solve(std::istream& in, std::ostream& out) {
	    int t;
	    in >> t;
	    while(t-- > 0) {
	        int a , b , c , d;
	        in >> a >> b >> c >> d;
	        if((a+c == b && a+c == d) || (a+d == b && a+d == c) || (b+c == a && b+c == d) || (b+d == a && b+d == c))
	            out << "YES\n";
	        else {
	            out << "NO\n";
	        }
	    }

	}
};


