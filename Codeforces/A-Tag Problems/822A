#include <bits/stdc++.h>

using namespace std;

class AImBoredWithLife {
public:
	void solve(std::istream& in, std::ostream& out) {
	    long long int a , b , ans = 1 , i ;
	    in >> a >> b ;
	    if(a > b) {
	        swap(a,b) ;
	    }
	    for(i = 1 ; i <= a ; i++) {
	        ans = ans*i ;
	    }
	    out << ans << "\n" ;

	}
};


int main() {
	AImBoredWithLife solver;
	std::ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
