using namespace std;

class AQAQ {
public:
	void solve(std::istream& in, std::ostream& out) {
	    string s;
	    in >> s;
	    long long int i , j , k ,ans = 0 , n = s.size();
	    for(i = 0 ; i < n ; i++) {
	        for(j = i+1 ; j < n ; j++) {
	            for(k = j+1 ; k < n ; k++) {
	                if(s[i] == 'Q' && s[j] == 'A' && s[k] == 'Q') {
	                    ans++;
	                }
	            }
	        }
	    }
	    out << ans << "\n";

	}
};


int main() {
	AQAQ solver;
	std::ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
