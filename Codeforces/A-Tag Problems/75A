#pragma comment(linker, "/stack:200000000")
#pragma GCC optimize("Ofast")
#pragma GCC target("sse,sse2,sse3,ssse3,sse4,popcnt,abm,mmx,avx,tune=native")

#include <bits/stdc++.h>

using namespace std;

class ALifeWithoutZeros {
public:
	void solve(std::istream& in, std::ostream& out) {
	    long long int a , b , i ;
	    in >> a >> b ;
	    long long int ans = a + b ;
	    string s1 = to_string(a) , s2 = to_string(b) , s = to_string(ans) ;
	    for(i = 0 ; i < s1.size() ; i++) {
	        if(s1[i] == '0') {
	            s1.erase(i,1) ;
	            i-- ;
	        }
	    }
        for(i = 0 ; i < s2.size() ; i++) {
            if(s2[i] == '0') {
                s2.erase(i,1) ;
                i-- ;
            }
        }
        for(i = 0 ; i < s.size() ; i++) {
            if(s[i] == '0') {
                s.erase(i,1) ;
                i-- ;
            }
        }
        a = stoi(s1) ;
        b = stoi(s2) ;
        ans = stoi(s) ;
        if(a+b == ans) {
            out << "YES\n" ;
        }
        else {
            out << "NO\n" ;
        }
	}
};


int main() {
	ALifeWithoutZeros solver;
	std::ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
	return 0;
}
