#pragma GCC optimize("Ofast")
#pragma GCC target("sse,sse2,sse3,ssse3,sse4,popcnt,abm,mmx,avx,tune=native")

#include <bits/stdc++.h>

using namespace std;

class APointsInSegments {
public:
	void solve(std::istream& in, std::ostream& out) {
	    int n , m , i , ans = 0;
	    in >> n >> m;
        int a[m+5];
        for(i = 0 ; i < m+3 ; i++) {
            a[i] = 0;
        }
        for(i = 0 ; i < n ; i++) {
	        int x , y;
	        in >> x >> y;
	        a[x] += 1;
	        a[y+1] += -1;
	    }
        for(i = 1 ; i <= m ; i++) {
            a[i] = a[i-1] +a[i];
        }
        for(i = 1 ; i <= m ; i++) {
            if(a[i] == 0) {
                ans++;
            }
        }
        out << ans << "\n";
        for(i = 1 ; i <= m ; i++) {
            if(a[i] == 0) {
                out << i << " " ;
            }
        }
        out << "\n";
	}
};


int main() {
	APointsInSegments solver;
	std::ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	std::istream& in(std::cin);
	std::ostream& out(std::cout);
	solver.solve(in, out);
