#include<bits/stdc++.h>
using namespace std;

void solve() {
    long long int a[4];
    set<long long int> st;
    for(long long int i = 0 ; i < 4 ; i++) {
        cin >> a[i];
        st.insert(a[i]);
    }
    cout << 4 - st.size() << "\n";
}

int main() {
    long long int tc = 1;
    // cin >> tc;
    while(tc-- > 0) {
        solve();
    }
}
