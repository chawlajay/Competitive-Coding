class C1TheGreatJulyaCalendar {
public:
	void solve(std::istream& in, std::ostream& out) {
	    long long int n , i , j , ans = 0;
	    in >> n;
	    if(n == 0) {
	        out << 0 << "\n";
	        return;
	    }
	    while(n/10 != 0) {
	        string s = to_string(n);
	        long long int maxi = 0;
	        for(i = 0 ; i < s.size() ; i++) {
	            if((long long int)(s[i] - '0') > maxi) {
	                maxi = (long long int)(s[i]-'0');
	            }
	        }
	        n -= maxi;
	        ans++;
	    }
	    out << ans + 1 << "\n";
	}
};


