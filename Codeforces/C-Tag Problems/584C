	void solve(std::istream& in, std::ostream& out) {
	    long long int n , k , i , j , t , same = 0 ;
	    string s1 , s2 , ans ;
	    in >> n >> t >> s1 >> s2 ;
	    for(i = 0 ; i < n ; i++) {
	        if(s1[i] == s2[i]) {
	            same++ ;
	        }
	    }
	    k = n - t ;
	    if(k <= same) {
	        ans = s1 ;
	        for(i = 0 ; i < n ; i++) {
	            if(k > 0) {
	                if(s1[i] == s2[i]) {
	                    k-- ;
	                }
	                else {
	                    for(char c = 'a' ; c <= 'z' ; c++) {
	                        if(s1[i] != c && s2[i] != c) {
	                            ans[i] = c ;
	                            break;
	                        }
	                    }
	                }
	            }
	            else {
                    for(char c = 'a' ; c <= 'z' ; c++) {
                        if(s1[i] != c && s2[i] != c) {
                            ans[i] = c ;
                            break;
                        }
                    }
	            }
	        }
	        out << ans ;
	    }
	    else {
	        if(2*(k - same) <= (n - same)) {
	            long long int a = k - same , b = k - same ;
	            ans = s1 ;
	            for(i = 0 ; i < n ; i++) {
	                if(s1[i] != s2[i]) {
	                    if(a > 0) {
	                        ans[i] = s1[i] ;
	                        a-- ;
	                    }
	                    else if(b > 0) {
	                        ans[i] = s2[i] ;
	                        b-- ;
	                    }
	                    else {
	                        for(char c = 'a' ; c <= 'z' ; c++) {
	                            if(s1[i] != c && s2[i] != c) {
	                                ans[i] = c ;
	                                break;
	                            }
	                        }
	                    }
	                }
	            }
	            out << ans << "\n" ;
	        }
	        else {
	            out << -1 << "\n" ;
	        }
	    }
	}
};


